{
    "name": "ampersand/array-merger",
    "description": "Merge arrays recusively using custom value choosers",
    "keywords": [
        "graze",
        "array-merger"
    ],
    "homepage": "https://github.com/graze/array-merger",
    "license": "MIT",
    "authors": [
        {
            "name": "Harry Bragg",
            "email": "harry.bragg@graze.com",
            "role": "Developer"
        },
        {
            "name": "Graze Developers",
            "email": "developers@graze.com",
            "homepage": "http://www.graze.com",
            "role": "Development Team"
        }
    ],
    "require": {
        "php": "^7.4||^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.7",
        "graze/standards": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Graze\\ArrayMerger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Graze\\ArrayMerger\\Test\\": "tests/src",
            "Graze\\ArrayMerger\\Test\\Unit\\": "tests/unit",
            "Graze\\ArrayMerger\\Test\\Integration\\": "tests/integration"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
